{
    baseKey:"CreateVolume.Block",
    serviceId:"CreateVolume",
    fields:{
        virtualArray:{
            type:"assetType.vipr.virtualArray",
            lockable: true
        },

        virtualPool:{
            type:"assetType.vipr.blockVirtualPool",
            lockable: true
        },

        project:{
            type:"assetType.vipr.project",
            lockable: true
        },

        volumes:{
          type:"table",
          items:{
            name:{
              type:"text",
              validation:{
                min:2,
                max:128
              }
            },
            size:{
              type:"storageSize",
              validation:{
                min:1
              }
            },
            numberOfVolumes:{
              type:"number",
              initialValue: "1",
              validation:{
                  min:1
              }
            }
          }
        },

        consistencyGroup:{
            type:"assetType.vipr.consistencyGroup",
            required:false
        }
    }

}
