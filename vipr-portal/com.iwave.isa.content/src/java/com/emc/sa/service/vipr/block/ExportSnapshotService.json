{
  baseKey:"ExportSnapshot.Block",
  serviceId:"ExportSnapshot",
  fields:{
    storageType:{
      type:"assetType.vipr.blockStorageType",
      lockable:true,
      initialValue:"exclusive"
    },

    host:{
      type:"assetType.vipr.host"
    },
    
    project: {
    	type:"assetType.vipr.project"
    },
    
    snapshots:{
    	type:"assetType.vipr.unassignedBlockSnapshot",
    	select: "many"
    },

    hlu: {
      type:"number",
      lockable: true,
      required: false,
      initialValue: "-1",
      validation: {
        min:-1
      }
    },
    
    advanced:{
      type:"group",
      collapsed:true,
      items:{        
        minPaths: {
          type:"number",
          lockable: true,
          required: false,
          validation: {
            min:1,
            max:65535
          }
        },
        
        maxPaths: {
          type:"number",
          lockable: true,
          required: false,
          validation: {
            min:1,
            max:65535
          }
        },
        
        pathsPerInitiator: {
          type:"number",
          lockable: true,
          required: false,
          validation: {
            min:1,
            max:65535
          }
        }
        
      }
    }    

  }
  
}
